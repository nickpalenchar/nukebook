{
  "manifest_version": 2,
  "name": "NukeBook",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>",
        "http://facebook.com/*",
        "https://www.facebook.com/*"
      ],
      "js": ["jquery-3.3.1.min.js", "es6-promise.min.js", "es6-promise.auto.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}